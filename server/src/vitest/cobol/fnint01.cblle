174400      PERFORM VARYING II FROM 1 BY 1 UNTIL II = NUM-OF-TEST + 1;
174500        PERFORM VARYING JJ FROM 1 BY 1 UNTIL JJ > 3;
174600          COMPUTE
174700               ACTUAL-RESULT-C1 =
174800               FUNCTION INTEGER(TEST-VALUEC1(II, JJ));
174900          IF ACTUAL-RESULT-C1 =
175000                     EXPECTED-RESULTSC1(II) THEN
175100             ADD 1 TO pass-count
175200          ELSE
175300             DISPLAY "COMP-1 TEST " II "," JJ " FAILED:"
175400             DISPLAY "test-value =      " TEST-VALUEC1(II, JJ)
175500             DISPLAY "expected-result = " EXPECTED-RESULTSC1(II)
